{
  "name": "wf-hellboy",
  "author": "innokentiyt",
  "version": "1.1.0",
  "keywords": [
    "pebble-app"
  ],
  "private": true,
  "dependencies": {
    "pebble-clay": "^1.0.4"
  },
  "pebble": {
    "displayName": "Hellboy",
    "uuid": "5c4aaed1-e271-4004-b649-0b99dd3cc637",
    "sdkVersion": "3",
    "enableMultiJS": true,
    "targetPlatforms": [
      "aplite",
      "basalt",
      "diorite"
    ],
    "watchapp": {
      "watchface": true
    },
    "capabilities": [
      "configurable"
    ],
    "messageKeys": [
      "show_am_pm",
      "show_leading_zero",
      "show_weekday"
    ],
    "resources": {
      "media": [
        {
          "type": "bitmap",
          "name": "BACKGROUND",
          "file": "images/background.png"
        },
        {
          "type": "bitmap",
          "name": "BT_ICON",
          "file": "images/bt.png"
        },
        {
          "type": "bitmap",
          "name": "AM_ICON",
          "file": "images/am.png"
        },
        {
          "type": "bitmap",
          "name": "PM_ICON",
          "file": "images/pm.png"
        },
        {
          "type": "bitmap",
          "name": "MENU_ICON",
          "file": "images/icon.png",
          "menuIcon": true
        },
        {
          "type": "font",
          "name": "JOST_MEDIUM_34",
          "file": "fonts/Jost-Medium.ttf",
          "compatibility": "2.7"
        }
      ]
    }
  }
}
